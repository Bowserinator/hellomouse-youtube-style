{
    "name" : "Hellomouse Youtube Style",
    "version": "0.0.1",
    "manifest_version": 2,
    "description" : "Better youtube style",
    "permissions": ["tabs", "http://*/*", "https://*/*", "file:///*/*"],
    "content_scripts" : [
        {
            "js" : ["init.js"],
            "css": ["css/desc.css", "css/unround.css", "css/ui.css"],
            "matches" : ["https://*.youtube.com/*"]
        }
    ]
}