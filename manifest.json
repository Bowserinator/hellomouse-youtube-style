{
    "name" : "Hellomouse Youtube Style",
    "version": "0.0.1",
    "manifest_version": 2,
    "description" : "Better youtube style",
    "permissions": ["http://*/*", "https://*/*"],
    "content_scripts" : [
        {
            "js" : ["init.js"],
            "css": ["css/desc.css", "css/unround.css", "css/ui.css", "css/optional.css"],
            "matches" : ["https://*.youtube.com/*"],
            "all_frames": true
        }
    ]
}